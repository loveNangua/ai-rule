[URL Rewrite]
^https?:\/\/[^\/]*\.jmapiproxy1\.cc\/static\/.+ - reject-img
^https?:\/\/[^\/]*\.jmdanjonproxy\.(?:vip|xyz)\/static\/.+ - reject-img

[MITM]
hostname = %APPEND% *.jmapiproxy1.cc, *.jmdanjonproxy.vip, *.jmdanjonproxy.xyz
