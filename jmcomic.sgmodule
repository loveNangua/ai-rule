#!name=Ad Static-Only (MITM)
#!desc=Block only /static/* on jmapiproxy1.cc & jmdanjonproxy.vip with image stub

[URL Rewrite]
# 仅拦截 static 路径（图片/素材），返回占位图，避免白屏或错误
^https?:\/\/cdn-msp\d+\.jmapiproxy1\.cc\/static\/.* - reject-img
^https?:\/\/cdn-msp\d+\.jmdanjonproxy\.vip\/static\/.* - reject-img

[MITM]
hostname = %APPEND% cdn-msp*.jmapiproxy1.cc, cdn-msp*.jmdanjonproxy.vip
